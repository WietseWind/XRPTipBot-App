{
  "name": "XRPTipBot",
  "version": "1.5.4",
  "private": true,
  "scripts": {
    "start": "node node_modules/react-native/local-cli/cli.js start",
    "start:android": "node node_modules/react-native/local-cli/cli.js run-android",
    "start:ios": "node node_modules/react-native/local-cli/cli.js run-ios",
    "install:android": "cd android && ./gradlew installDebug",
    "release:android": "npm run bundle:android && cd android && ./gradlew assembleRelease",
    "bundle:ios": "node ./node_modules/react-native/local-cli/cli.js bundle --platform ios --entry-file index.js --bundle-output ios/XRPTipBot/main.jsbundle --dev=false --verbose --assets-dest ios/XRPTipBot",
    "bundle:android": "node ./node_modules/react-native/local-cli/cli.js bundle --platform android --dev false --entry-file index.js --bundle-output android/app/src/main/assets/index.android.bundle",
    "prettier": "prettier --write `find src -name \"*.js\"`",
    "version": "./scripts/version-ios.sh",
    "postinstall": "./scripts/postinstall.sh"
  },
  "dependencies": {
    "@expo/react-native-action-sheet": "^2.1.0",
    "lottie-react-native": "^2.6.1",
    "moment": "^2.24.0",
    "moment-timezone": "^0.5.25",
    "qr.js": "0.0.0",
    "ramda": "^0.26.1",
    "react": "16.8.3",
    "react-native": "0.59.5",
    "react-native-camera": "^2.6.0",
    "react-native-device-info": "^1.5.0",
    "react-native-discovery": "https://github.com/N3TC4T/react-native-discovery",
    "react-native-firebase": "^5.3.1",
    "react-native-interactable": "^0.1.10",
    "react-native-linear-gradient": "^2.5.4",
    "react-native-navigation": "1.1.486",
    "react-native-root-siblings": "^3.1.7",
    "react-native-share": "^1.1.3",
    "react-native-view-shot": "^2.5.0",
    "react-redux": "^6.0.1",
    "redux": "^4.0.1",
    "redux-logger": "^3.0.6",
    "redux-persist": "^4.10.2",
    "redux-thunk": "^2.3.0",
    "seamless-immutable": "^7.1.4"
  },
  "devDependencies": {
    "@babel/core": "^7.3.3",
    "@babel/runtime": "^7.3.1",
    "@react-native-community/eslint-config": "^0.0.3",
    "babel-jest": "^24.1.0",
    "babel-plugin-transform-remove-console": "^6.9.4",
    "eslint": "^5.16.0",
    "eslint-plugin-prettier": "^3.0.1",
    "jest": "^24.1.0",
    "metro-react-native-babel-preset": "^0.51.1",
    "prettier": "^1.17.0",
    "react-test-renderer": "16.8.1",
    "remote-redux-devtools": "^0.5.16"
  },
  "jest": {
    "preset": "react-native"
  }
}
