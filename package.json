{
  "name": "XRPTipBot",
  "version": "1.4.1",
  "private": true,
  "scripts": {
    "start": "node node_modules/react-native/local-cli/cli.js start",
    "start:android": "node node_modules/react-native/local-cli/cli.js run-android",
    "start:ios": "node node_modules/react-native/local-cli/cli.js run-ios",
    "install:android": "cd android && ./gradlew installDebug",
    "release:android": "cd android && ./gradlew assembleRelease",
    "bundle:ios": "node ./node_modules/react-native/local-cli/cli.js bundle --platform ios --entry-file index.js --bundle-output ios/XRPTipBot/main.jsbundle --dev=false --verbose --assets-dest ios/XRPTipBot",
    "bundle:android": "node ./node_modules/react-native/local-cli/cli.js bundle --platform android --dev false --entry-file index.js --bundle-output android/app/src/main/assets/index.android.bundle",
    "postinstall": "sh ./postinstall.sh"
  },
  "dependencies": {
    "@expo/react-native-action-sheet": "^1.1.0",
    "lottie-react-native": "^2.5.8",
    "moment": "^2.22.2",
    "moment-timezone": "^0.5.23",
    "qr.js": "0.0.0",
    "ramda": "^0.25.0",
    "react": "16.5.0",
    "react-native": "0.57.0",
    "react-native-camera": "^1.3.0",
    "react-native-device-info": "^0.22.5",
    "react-native-firebase": "^5.1.0",
    "react-native-interactable": "^0.1.10",
    "react-native-linear-gradient": "^2.4.0",
    "react-native-navigation": "1.1.484",
    "react-native-root-siblings": "^3.1.4",
    "react-native-share": "^1.1.2",
    "react-native-view-shot": "^2.4.0",
    "react-redux": "^5.0.6",
    "redux": "^4.0.0",
    "redux-logger": "^3.0.6",
    "redux-persist": "^4.10.2",
    "redux-thunk": "^2.0.1",
    "seamless-immutable": "^7.1.2"
  },
  "devDependencies": {
    "babel-plugin-transform-remove-console": "^6.9.4",
    "remote-redux-devtools": "^0.5.12"
  }
}
